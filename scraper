    if (localStorage.getItem('theme-mode') == 'darkmode') {
        document.querySelectorAll('.switch > input').forEach(function(el) {
            el.checked = true;
        });
    } else {
        document.querySelectorAll('.switch > input').forEach(function(el) {
            el.checked = false;
        });
    }
